{
  "source": {
    "include": "./lib",
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": ".+\\.test.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknowTags": true,
    "dectionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/"
  }
}
